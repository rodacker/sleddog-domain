{
    "name": "rodacker/sleddog-domain",
    "description": "Domain model for sleddogs",
    "type": "library",
    "authors": [
        {
            "name": "Patrick Rodacker",
            "email": "patrick@rodacker.de"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "ramsey/uuid": "^4.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Rodacker\\Sleddog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rodacker\\Sleddog\\Test\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "php vendor/bin/phpunit",
        "phpunit-coverage": "php vendor/bin/phpunit --coverage-text"
    }
}
